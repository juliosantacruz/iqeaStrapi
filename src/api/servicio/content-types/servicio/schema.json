{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "servicio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "text"
    },
    "contenido": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "required": true,
      "customField": "plugin::ckeditor.CKEditor"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
